{
  "name": "@infernus/core",
  "version": "0.9.4",
  "description": "NodeJS library for Scripting Open Multiplayer",
  "keywords": [
    "omp",
    "samp"
  ],
  "homepage": "https://github.com/dockfries/infernus/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/dockfries/infernus/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dockfries/infernus.git"
  },
  "license": "MIT",
  "author": "dockfries",
  "exports": {
    ".": {
      "require": "./dist/bundle.js",
      "import": "./dist/bundle.mjs"
    }
  },
  "main": "dist/bundle.js",
  "module": "dist/bundle.mjs",
  "types": "dist/bundle.d.ts",
  "files": [
